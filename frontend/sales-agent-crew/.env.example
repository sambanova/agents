# Frontend Environment Variables Example

# API Configuration
VITE_API_URL=/api
VITE_WEBSOCKET_URL=ws://localhost:8000

# Feature Flags
VITE_ENABLE_WORKFLOW_TOGGLE=false
VITE_ENABLE_USER_KEYS=false

# Auth0 Configuration
VITE_AUTH0_SCOPES=openid profile email
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-client-id

# Admin Panel Settings
# Set to true to enable the LLM provider configuration admin panel
# Must also be set to true in backend (.env SHOW_ADMIN_PANEL=true)
VITE_SHOW_ADMIN_PANEL=false